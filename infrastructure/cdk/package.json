{
  "name": "@ai-compliance-shepherd/cdk-demo",
  "version": "1.0.0",
  "description": "AWS CDK Demo for AI Compliance Shepherd Platform",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy --app 'npx ts-node app-simple.ts'",
    "diff": "cdk diff --app 'npx ts-node app-simple.ts'",
    "synth": "cdk synth --app 'npx ts-node app-simple.ts'",
    "destroy": "cdk destroy --app 'npx ts-node app-simple.ts'"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.150.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "aws-cdk",
    "infrastructure",
    "compliance",
    "security",
    "demo"
  ],
  "author": "AI Compliance Shepherd Team",
  "license": "MIT"
}
